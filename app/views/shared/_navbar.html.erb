<div class="navbar navbar-expand-sm navbar-light navbar-lewagon">
  <div>
  <%= link_to root_path, class: "navbar-brand" do %>
    <img src="data:image/svg+xml;base64," />
  <% end %>
  <%= link_to 'geoDengue', root_path, class: 'navbar-brand text-primary', id: 'brand' %>
  </div>

  <button class="navbar-toggler" type="button" data-toggle="collapse" 
  data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" 
  aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>


  <div class="collapse navbar-collapse" id="navbarSupportedContent">
    <ul class="navbar-nav mr-auto">
      <% if user_signed_in? %>
        <li class="nav-item active d-md-none">
          <%= link_to "Registrar visita", new_place_path, class: "nav-link" %>
        </li>
        <li class="nav-item d-md-none">
          <%= link_to "Visualizar minha área", places_path, class: "nav-link" %>
        </li>
        <li class="nav-item d-md-none">
          <%= link_to "Dashboard", dashboard_path, class: "nav-link" %>
        </li>
        <li class="nav-item d-md-none">
          <%= link_to "Log out", destroy_user_session_path, method: :delete, class: "nav-link" %>
        </li>

        <li class="nav-item dropdown">
          <%= image_tag "oscar.jpg", class: "avatar dropdown-toggle", 
          id: "navbarDropdown", data: { toggle: "dropdown" }, 'aria-haspopup': true, 'aria-expanded': false %>
          <div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdown">
          <%# d-none d-md-block não aparece no mobile, mas não faz dropdown no grande %>
            <%= link_to "Registrar visita", new_place_path, class: "dropdown-item" %>
            <%= link_to "Visualizar minha área", places_path, class: "dropdown-item" %>
            <%= link_to "Dashboard", dashboard_path, class: "dropdown-item" %>
            <%= link_to "Log out", destroy_user_session_path, method: :delete, class: "dropdown-item" %>
          </div>
        </li>
      <% else %>
        <li class="nav-item">
          <%= link_to "Login", new_user_session_path, class: "nav-link" %>
        </li>
      <% end %>
    </ul>
  </div>
</div>
